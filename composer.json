{
    "name": "r4j4h/jspackager-cli",
    "description": "Basic console for jspackager-test.",
    "keywords": ["dependency resolution", "file inclusion", "compilation"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:r4j4h/jspackager-test"
        }
    ],
    "require": {
        "psr/log": "1.0.0",
        "r4j4h/jspackager-test": "~2.0@rc",
        "symfony/console": "2.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "phing/phing": "2.*",
        "phpmd/phpmd": "2.1.1",
        "phploc/phploc": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "JsPackager\\": "src/JsPackager/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsPackager\\": "tests/"
        }
    },
    "bin": ["bin/jspackager"],
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.x-dev",
            "dev-master": "1.0.x-dev"
        }
    }
}
